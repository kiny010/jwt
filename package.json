    "babel-preset-stage-2": "6.24.1",
    "babel-register": "6.24.1",
    "fs": "0.0.1-security",
    "jshint": "2.9.4",
    "mocha": "3.2.0",
    "nodemon": "^1.11.0",
    "webpack": "2.4.1",
    "webpack-dev-server": "2.4.2"
  },
  "dependencies": {
